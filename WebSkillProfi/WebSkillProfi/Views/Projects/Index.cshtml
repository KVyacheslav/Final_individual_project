@model IEnumerable<Project>

<section class="u-align-left u-clearfix u-section-3" id="carousel_a34f">
    <div class="u-clearfix u-sheet u-sheet-1">
        <h2 class="u-align-left u-custom-font u-font-montserrat u-text u-text-default u-text-1">
            Проекты<br>
        </h2>
        <div class="u-expanded-width u-list u-list-1">
            <div class="u-repeater u-repeater-1">

                @foreach (var item in Model)
                {
                    <div class="u-align-left u-border-1 u-border-grey-75 u-container-style u-list-item u-palette-5-light-2 u-repeater-item u-shape-rectangle u-list-item-1">
                        <div class="u-container-layout u-similar-container u-valign-top u-container-layout-1">
                            <img class="u-expanded-width u-image u-image-default u-image-1" src="~/@item.ImageUrl" alt="" data-image-width="1400" data-image-height="644">
                            <p class="u-text u-text-2">@item.Title</p>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</section>